function newtonInterpFn
x = [1 2 3 4];
y = [1 4 9 16];
tointerp = 1.5;
d = divdif(x,y);
n = length(x);

function v = newtVal(z,x,d,i)
if i == 1
	v = d(1);
else
	v = newtVal(z,x,d,i-1) + d(i) * basis(z,x,i);
end

function v = basis(z,x,i)
if i == 1
	v = 1;
else
	v = basis(z,x,i-1) * (z - x(i));
end


