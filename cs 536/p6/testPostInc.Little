void main() {
  int x;
  int y;
  int z;
  x = 1;
  z = 2;
  y = x++ + z;
  printf("y should be 3\n");
  printf("y: ");
  printf("%d", y);
  printf("\n");
  printf("x should be 2\n");
  printf("x: ");
  printf("%d", x);
  printf("\n");
  y = z + x++;
  printf("y should be 4\n");
  printf("y: ");
  printf("%d", y);
  printf("\n");
  printf("x should be 3\n");
  printf("x: ");
  printf("%d", x);
  printf("\n");
}
