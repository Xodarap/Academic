
void main(->void; offset: -1)() {
  int x(int; offset: -1);
  double d(double; offset: -1);
  scanf("%d", &x(int; offset: -1));
  printf("x: ");
  printf("%d", x(int; offset: -1));
  printf("\n");
  scanf("%f", &d(double; offset: -1));
  printf("d: ");
  printf("%d", d(double; offset: -1));
  printf("\n");
}
