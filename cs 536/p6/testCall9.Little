/* TEST INT AND DOUBLE ACTUALS AND FORMALS */

void f(double a, int b, double c, int d) {
  printf("Expected output:\n1.5 2 3.5 4\n");
  printf("%f", a);
  printf(" ");
  printf("%d", b);
  printf(" ");
  printf("%f", c);
  printf(" ");
  printf("%d", d);
  printf("\n");
  return;
}

int g;
void main() {
  double x;
  int y;
  double z;
  x = 1.5;
  y = 2;
  z = 3.5;
  g = 4;
  f(x, y, z, g);
}
